    "autoprefixer-loader": "^3.1.0",
    "babel-core": "^6.1.21",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "chai": "^3.3.0",
    "chai-immutable": "^1.5.3",
    "css-loader": "^0.19.0",
